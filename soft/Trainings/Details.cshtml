@page
@using isa3.Core.Extensions
@model isa3.soft.DetailsModel

@{
    Layout = "_Details";
    ViewData["ItemId"] = Model.TrainingData.Id;
    ViewData["Page"] = "Trainings";
}

@Html.Show(model => model.TrainingData.Name)
@Html.Show(model => model.TrainingData.Description)
@Html.Show(model => model.TrainingData.LocationOfTraining)
@Html.Show(model => model.TrainingData.AvailableEquipment)
@Html.Show(model => model.TrainingData.MaxNumberOfAthletes)
@Html.Show(model => model.TrainingData.MinNumberOfAthletes)
@Html.Show(model => model.TrainingData.NumberOfAthletes)
@Html.Show(model => model.TrainingData.NumberOfCoachesRequired)
@Html.Show(model => model.TrainingData.ValidFrom)
@Html.Show(model => model.TrainingData.ValidTo)
